              <class name="stability"/>
            </style>
            <property name="valign">center</property>
            <property name="visible">false</property>
            <binding name="value">
              <lookup name="stability" type="ManualsKeyword">
                <lookup name="item" type="ManualsNavigatable">
                  <lookup name="item" type="ManualsSearchResult">
                    <lookup name="item">GtkListItem</lookup>
                  </lookup>
                </lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="ManualsTag" id="language">
            <style>
              <class name="language"/>
            </style>
            <property name="valign">center</property>
            <property name="visible">false</property>
            <property name="key">language</property>
            <binding name="value">
              <closure function="format_language" type="gchararray">
                <lookup name="language" type="ManualsBook">
                  <lookup name="item" type="ManualsNavigatable">
                    <lookup name="item" type="ManualsSearchResult">
                      <lookup name="item">GtkListItem</lookup>
                    </lookup>
                  </lookup>
                </lookup>
              </closure>
            </binding>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
