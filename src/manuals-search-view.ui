                  <lookup name="item">GtkListItem</lookup>
                </lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="ManualsTag" id="deprecated">
            <style>
              <class name="deprecated"/>
            </style>
            <property name="valign">center</property>
            <!-- translators: lowercase to match gi-docgen generated documentation -->
            <property name="key" translatable="yes">deprecated</property>
            <property name="visible">false</property>
            <binding name="value">
              <lookup name="deprecated" type="ManualsKeyword">
                <lookup name="item" type="ManualsNavigatable">
                  <lookup name="item" type="ManualsSearchResult">
                    <lookup name="item">GtkListItem</lookup>
                  </lookup>
                </lookup>
              </lookup>
            </binding>
          </object>
        </child>
